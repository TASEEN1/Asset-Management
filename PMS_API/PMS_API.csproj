<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>disable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="wwwroot\Report\Report Dataset\AssetDataSet.xsc" />
    <Content Remove="wwwroot\Report\Report Dataset\AssetDataSet.xsd" />
    <Content Remove="wwwroot\Report\Report Dataset\AssetDataSet.xss" />
    <Content Remove="wwwroot\Report\Asset_report\AssetDetailsSummary.rdlc" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="wwwroot\Report\Asset_report\AssetDetailsSummary.rdlc" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="CoreApiResponse" Version="1.0.1" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="6.0.21" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="6.0.21" />
    <PackageReference Include="Microsoft.Data.SqlClient" Version="5.1.1" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.5.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\PMS_DAL\PMS_DAL.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Content Update="wwwroot\Report\Cutting_Report\CuttingDataSet.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>CuttingDataSet.xsd</DependentUpon>
    </Content>
    <Content Update="wwwroot\Report\Cutting_Report\CuttingDataSet.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>CuttingDataSet.Designer.cs</LastGenOutput>
    </Content>
    <Content Update="wwwroot\Report\Report Dataset\AssetDataSet.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>AssetDataSet.xsd</DependentUpon>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <None Include="wwwroot\Report\Report Dataset\AssetDataSet.xsc" />
    <None Include="wwwroot\Report\Report Dataset\AssetDataSet.xsd">
      <SubType>Designer</SubType>
      <LastGenOutput>AssetDataSet.Designer.cs</LastGenOutput>
      <Generator>MSDataSetGenerator</Generator>
    </None>
    <None Include="wwwroot\Report\Report Dataset\AssetDataSet.xss" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Mr_Asset_InfoDataSet.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Mr_Asset_InfoDataSet.xsd</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <None Update="Mr_Asset_InfoDataSet.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>Mr_Asset_InfoDataSet.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>
</Project>
